{
    "gnesis-model-with-multiple": {
        "prefix": "gm",
        "body": [
            "module.exports = function(sequelize, DataTypes) {",
            "  const attributes = {",
            "      id: {",
            "           type: DataTypes.INTEGER,",
            "           primaryKey: true,",
            "           autoIncrement: true,",
            "           pintar: [2,3,4],",
            "           name: 'Identificador',",
            "           tipo: 'number',",
            "       },",
            "  };",
            "",
            "  const options = {",
            "    freezeTableName: true,",
            "    paranoid: true,",
            "    referencia: {",
            "      titulo_modelo: '${2:Titulo de modelo}',",
            "      representante: ['${3:id}']",
            "    },",
            "    classMethods: {",
            "      associate: function(models) {",
            "        ${1:NombreModelo}.belongsTo(models.Usuario, {",
            "          as: 'Creador',",
            "          foreignKey: {",
            "            allowNull: false",
            "          }",
            "        });",
            "      }",
            "    },",
            "    multiples: [",
            "    ],",
            "    seguridad: {",
            "      1: '${1:NombreModelo}Ins',",
            "      2: '${1:NombreModelo}Act',",
            "      3: '${1:NombreModelo}Eli',",
            "      4: '${1:NombreModelo}Bus'",
            "    }",
            "  };",
            "",
            "  var ${1:NombreModelo} = sequelize.define('${1:NombreModelo}', attributes, options);",
            "  return ${1:NombreModelo};",
            "}"
        ],
        "description": "Snipet para generar un modelo con multiples en gnesis"
    },
